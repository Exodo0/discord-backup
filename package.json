    "author": "Egologic",
    "packageManager": "pnpm@10.29.2",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Exodo0/discord-backup/issues"
    },
    "homepage": "https://github.com/Exodo0/discord-backup#readme",
    "dependencies": {
        "discord.js": "^14.25.1",
        "mongoose": "^8.23.0"
    },
    "devDependencies": {
        "@types/node": "^24.10.13",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint-config-prettier": "^9.1.2",
        "prettier": "^3.8.1",
        "turbo": "^2.8.7",
        "typescript": "^5.9.3"
    }
